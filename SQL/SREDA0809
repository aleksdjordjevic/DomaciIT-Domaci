SELECT PROFESORID
FROM PROFESORI
WHERE GODINE > 45;

SELECT IME, PREZIME
FROM UCENICI
WHERE GODINE > 15 AND PROSEK > 6.5;

SELECT GODINE
FROM UCENICI
ORDER BY UCENICI ASC;

SELECT PREDMET
FROM PROFESORI
WHERE PREDMET = 'INFORMATIKA';

SELECT UCENIKID, IME, PREZIME FROM UCENICI
WHERE IME LIKE '______';



--RESENJE PRVOG
SELECT PHONE_NUMBER, EMAIL 
FROM EMPLOYEES 
WHERE
 PHONE_NUMBER IS NOT NULL AND (DEPARTMENT_ID IN (SELECT  DEPARTMENT_ID
FROM DEPARTMENTS
WHERE LOCATION_ID IN (SELECT LOCATION_ID
FROM locations
WHERE COUNTRY_ID IN (SELECT COUNTRY_ID
FROM countries
WHERE REGION_ID =
(SELECT REGION_ID
FROM REGIONS
WHERE REGION_NAME = 'Europe')
))
)  )

--RESENJE DRUGOG
SELECT first_name, LAST_NAME
FROM DEPENDENTS 
WHERE EMPLOYEE_ID IN(SELECT EMPLOYEE_ID   
FROM EMPLOYEES 
ORDER BY SALARY  DESC 
LIMIT 5)



